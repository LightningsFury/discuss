{
  "presets": [
    "@babel/env",
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    "@babel/plugin-proposal-partial-application",
    "@babel/plugin-proposal-decorators",
    "@babel/plugin-proposal-optional-chaining"
  ]
}